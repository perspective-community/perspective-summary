#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/perspective-summary/perspective-summary/node_modules/.pnpm/pro_self_extracting_wasm@0.0.9/node_modules/pro_self_extracting_wasm/node_modules:/home/runner/work/perspective-summary/perspective-summary/node_modules/.pnpm/pro_self_extracting_wasm@0.0.9/node_modules:/home/runner/work/perspective-summary/perspective-summary/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/perspective-summary/perspective-summary/node_modules/.pnpm/pro_self_extracting_wasm@0.0.9/node_modules/pro_self_extracting_wasm/node_modules:/home/runner/work/perspective-summary/perspective-summary/node_modules/.pnpm/pro_self_extracting_wasm@0.0.9/node_modules:/home/runner/work/perspective-summary/perspective-summary/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --max-old-space-size=8192 "$basedir/../../../../../../pro_self_extracting_wasm@0.0.9/node_modules/pro_self_extracting_wasm/main.mjs" "$@"
else
  exec node  --max-old-space-size=8192 "$basedir/../../../../../../pro_self_extracting_wasm@0.0.9/node_modules/pro_self_extracting_wasm/main.mjs" "$@"
fi
